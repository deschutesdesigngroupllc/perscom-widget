{
  "name": "perscom-3.0-widget",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:widget": "parcel build src/widget.js",
    "test": "react-scripts test",
    "lint": "eslint --fix 'src/**/*.js' './*.js'",
    "format": "prettier 'src/**/*.js' './*.js' --write --config ./.prettierrc"
  },
  "dependencies": {
    "@sentry/react": "^7.34.0",
    "@sentry/tracing": "^7.34.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@parcel/compressor-brotli": "^2.8.3",
    "@parcel/compressor-gzip": "^2.8.3",
    "@heroicons/react": "^2.0.14",
    "classnames": "^2.3.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "iframe-resizer": "4.3.3",
    "lodash": "^4.17.21",
    "object-hash": "^3.0.0",
    "parcel": "^2.8.3",
    "parcel-namer-rewrite": "^2.0.0-rc.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "process": "^0.11.10",
    "tailwindcss": "^3.2.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "parcel-namer-rewrite": {
    "chain": "@parcel/namer-default",
    "hashing": "never",
    "rules": {
      "index.js": "app.js",
      "index.css": "app.css",
      "loader.js": "widget.js"
    }
  }
}
